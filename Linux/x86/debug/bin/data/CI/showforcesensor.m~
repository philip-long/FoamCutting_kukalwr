function showforcesensor()
data = load('forcesensor');

figure(1)
title("Force");

hold on;
plot(data(:,1),'y');
hold on;
plot(data(:,2),'m');
hold on;
plot(data(:,3),'c');
hold on;


legend('X','Y','Z')


figure(2)
title("Moments");

plot(data(:,4),'y');
hold on;
plot(data(:,5),'m');
hold on;
plot(data(:,6),'c');
hold on;
legend('x(1)','y(2)','z(3)');

